{
	"compileOnSave": false,
	"compilerOptions": {
		"allowJs": true,
		"alwaysStrict": true,
		"declaration": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"forceConsistentCasingInFileNames": true,
		"lib": [
			"es2017",
			"dom"
		],
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedParameters": true,
		"outDir": "./dist/out-tsc",
		"pretty": true,
		"sourceMap": true,
		"target": "es5",
		"typeRoots": [
			"node_modules/@types"
		]
	}
}
